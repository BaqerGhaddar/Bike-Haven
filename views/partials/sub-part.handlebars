<div class='sub-part-card'>
{{#if this}}
  <div class='sub-part-header-wrapper'>
  
    <h3 class='sub-part-header'>{{part.name}}</h3>
  </div>
  <div class='sub-part-wrapepr'>
    <span>Quality: {{part.quality_type}}</span>
  </div>

  <div class='sub-part-wrapepr'>
    <img
      src='/images/parts/{{vfilename}}'
      class='img-thumbnail img img-responsive'
    />
  </div>

  <div class='sub-part-wrapepr'>
    <span class='btn btn-outline-dark btn-block'>Price: {{part.price}}</span>
  </div>
  {{/if}}
</div>