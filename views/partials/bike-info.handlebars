<div class='bike-card' data-id='{{id}}'>
  <div id='bike_id'>
    <button class='wishlist-btn'> + Add to Wishlist</button>
    <div id='est_title'>{{brand}} <b>{{model}}</b> {{id}}</div>
    {{#if this}}
      <img
        src='/images/bikes/{{filename}}'
        class='img-thumbnail img img-responsive'
      />
      <span>Price: {{price}}</span>
    {{/if}}
  </div>
</div>

<script src='/javascript/add-item.js'></script>